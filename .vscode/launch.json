{
    "version": "0.2.0",
    "configurations": [
       {
         "type": "node",
         "request": "launch",
         "name": "Launch with nodemon",
         "runtimeExecutable": "npm",
         "runtimeArgs": [
           "run-script",
           "dev"
         ],
         "envFile": "${workspaceFolder}/.env",
         "console": "integratedTerminal",
         "internalConsoleOptions": "neverOpen"
       }
    ]
   }
   